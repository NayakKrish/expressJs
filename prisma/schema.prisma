generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model hotels {
  id          Int     @id @default(autoincrement())
  name        String  @db.VarChar(255)
  slug        String? @db.VarChar(255)
  vendor_name String  @db.VarChar(255)
}
